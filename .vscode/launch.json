{
  "version": "0.2.0",
  "configurations": [
    {
      // IntelliSense verwenden, um herauszufinden, welche Attribute für das C#-Debuggen vorhanden sind
      // Hover für die Beschreibung der vorhandenen Attribute verwenden
      // Weitere Informationen finden Sie unter https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
      "name": ".NET Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      // Wenn Sie Zielframeworks geändert haben, stellen Sie sicher, dass Sie den Programmpfad aktualisieren.
      "program": "${workspaceFolder}/BlazorBp/bin/Debug/net9.0/BlazorBp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/BlazorBp",
      "stopAtEntry": false,
      // Aktivieren Sie das Starten eines Webbrowsers, wenn ASP.NET Core gestartet wird. Weitere Informationen: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      // IntelliSense verwenden, um herauszufinden, welche Attribute für das C#-Debuggen vorhanden sind
      // Hover für die Beschreibung der vorhandenen Attribute verwenden
      // Weitere Informationen finden Sie unter https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md.
      "name": ".NET Launch (ID-web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      // Wenn Sie Zielframeworks geändert haben, stellen Sie sicher, dass Sie den Programmpfad aktualisieren.
      "program": "${workspaceFolder}/BlazorBp.Identity/bin/Debug/net9.0/BlazorBp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/BlazorBp.Identity",
      "stopAtEntry": false,
      // Aktivieren Sie das Starten eines Webbrowsers, wenn ASP.NET Core gestartet wird. Weitere Informationen: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}